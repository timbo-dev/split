{
    "name": "split",
    "private": true,
    "version": "0.1.1",
    "type": "module",
    "main": "src/main.tsx",
    "lint-staged": {
        "*.ts": [
            "npm run staged-lint",
            "npm run staged-tests"
        ]
    },
    "config": {
        "commitizen": {
            "path": "timbo-cz-conventional-changelog"
        }
    },
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "prepare": "husky install",
        "remove-build": "rimraf ./build",
        "staged-lint": "eslint",
        "staged-tests": "npx vitest related --run",
        "test": "vitest",
        "preview": "vite preview"
    },
    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-multi-carousel": "^2.8.2",
        "react-router-dom": "^6.8.2",
        "styled-components": "^5.3.8",
        "vite-plugin-svgr": "^2.4.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.4.3",
        "@types/jest": "^29.4.0",
        "@types/node": "^18.13.0",
        "@types/react": "^18.0.27",
        "@types/react-dom": "^18.0.10",
        "@types/styled-components": "^5.1.26",
        "@typescript-eslint/eslint-plugin": "^5.54.1",
        "@typescript-eslint/parser": "^5.54.1",
        "@vitejs/plugin-react-swc": "^3.0.0",
        "@vitest/coverage-c8": "^0.28.5",
        "chokidar": "^3.5.3",
        "commitizen": "^4.3.0",
        "commitlint": "^17.4.3",
        "commitlint-config-gitmoji": "^2.3.1",
        "eslint": "^8.34.0",
        "eslint-plugin-import-helpers": "^1.3.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-timbo-linter": "^0.1.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "husky": "^8.0.3",
        "inquirer": "8",
        "lint-staged": "^13.1.2",
        "timbo-cz-conventional-changelog": "^0.1.0",
        "tsconfig-paths": "^3.14.2",
        "typescript": "^4.9.3",
        "unplugin-auto-import": "^0.14.3",
        "vite": "^4.1.0",
        "vite-tsconfig-paths": "^4.0.5",
        "vitest": "^0.28.5"
    }
